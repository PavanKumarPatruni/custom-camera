<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="@dimen/size_fifty"
        android:height="@dimen/size_fifty">
        <shape android:shape="oval">

            <stroke
                android:width="@dimen/dimen_three"
                android:color="@color/color_white" />
            <solid android:color="@color/color_transparent" />

        </shape>
    </item>

    <item
        android:bottom="@dimen/dimen_three"
        android:left="@dimen/dimen_three"
        android:right="@dimen/dimen_three"
        android:top="@dimen/dimen_three">
        <shape android:shape="oval">

            <stroke
                android:width="@dimen/dimen_three"
                android:color="@color/color_transparent" />
            <solid android:color="@color/color_transparent_shade_3" />

        </shape>
    </item>

</layer-list>